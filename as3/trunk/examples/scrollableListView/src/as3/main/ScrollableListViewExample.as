package{	import ExampleScrollableListView;	import ExampleListItem;	import inky.framework.collections.ArrayList;	import inky.framework.components.listViews.IListView;	import inky.framework.components.listViews.scrollableListView.ScrollableListView;	import flash.display.DisplayObject;	import flash.display.Sprite;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	public class ScrollableListViewExample extends Sprite	{		/**		 *		 *		 *		 */			public function ScrollableListViewExample()		{			// Create a ScrollPane and give it content.			var listView:IListView = new ExampleScrollableListView();			this.addChild(listView as DisplayObject);			listView.listItemViewClass = ExampleListItem;			listView.dataProvider = new ArrayList([				{index: 0},				{index: 1},				{index: 2},				{index: 3},				{index: 4},				{index: 5},				{index: 6},				{index: 7},				{index: 8},				{index: 9}			]);						// Center the ListView. (Not important.. just for looks)			listView.x = 75;			listView.y = 25;		}	}}