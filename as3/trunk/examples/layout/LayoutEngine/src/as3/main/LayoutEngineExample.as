package  {	import flash.display.Sprite;	import inky.layout.LayoutEngine;	import inky.layout.GridLayout;	import flash.events.Event;	import Window;	/**	 *	 *  ..	 *		 * 	@langversion ActionScript 3	 *	@playerversion Flash 9.0.0	 *	 *	@author matthew	 *	@since  2009.07.30	 *	 */	public class LayoutEngineExample extends Sprite	{		/**		 *		 */		public function LayoutEngineExample()		{			var window:Window = new Window();			this.addChild(window);						// Add nested windows.			for (var i:int = 0; i < 4; i++)			{				var nestedWindow:Window = new Window();				window.addNestedWindow(nestedWindow);			}							window.addEventListener(Event.ENTER_FRAME, this._enterFrameHandler);		}private function _enterFrameHandler(event:Event):void{	event.currentTarget.width = this.mouseX;}			}	}