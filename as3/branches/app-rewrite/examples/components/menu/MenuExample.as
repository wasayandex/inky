package{	/**	 *	@author Ryan Sprake	 *	@since 2010.02.04	 */		import flash.display.Sprite;		import inky.components.menu.controllers.MenuController;	import inky.components.menu.events.MenuEvent;	import inky.collections.AbstractList;	import inky.collections.IList;	import inky.components.listViews.gridList.GridList;		import com.exanimo.controls.ButtonClip;		public class MenuExample extends Sprite	{		public function MenuExample()		{			var list:IList = new AbstractList();			list.addItem({label: "thing 1"});			list.addItem({label: "thing 2"});			list.addItem({label: "thing 3"});						var gridList:GridList = new GridList(10, 0, 10);			gridList.itemViewClass = MenuButton;			gridList.model = list;			this.addChild(gridList);						var dynamicMenu:MenuController = new MenuController(gridList);			dynamicMenu.addEventListener(MenuEvent.CHANGE, this._menuChangeHandler);						var staticMenu:MenuController = new MenuController(this.getChildByName("staticMenu"));			staticMenu.addEventListener(MenuEvent.CHANGE, this._menuChangeHandler);		}		//		// Get / Set Functions		//		//		// Public Functions		//		//		// Private Functions		//						/**		 */		private function _menuChangeHandler(event:MenuEvent):void		{			trace("-> _menuChangeHandler " + event);			trace("activeMenuItem " + event.currentTarget);		}	}}