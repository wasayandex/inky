package{	import inky.components.button.CommandButton;	import inky.commands.TweenerCommand;		import caurina.transitions.*;	import inky.binding.events.PropertyChangeEvent;		import flash.display.Sprite;		public class MyCommandButton extends CommandButton	{		public function MyCommandButton()		{			super();						this.over = new TweenerCommand({alpha: 1, time: 1, transition: "easeOutQuint"}, this);			this.up = new TweenerCommand({alpha: 0.5, time: 1, transition: "easeOutQuint"}, this);			this.selectedOver = new TweenerCommand({alpha: 0.75, time: 1, transition: "easeOutQuint"}, this);			this.selectedUp = new TweenerCommand({alpha: 1, time: 1, transition: "easeOutQuint"}, this);						this.toggle = true;				this.alpha = 0.5;		}	}}